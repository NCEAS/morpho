<launch4jConfig>
  <dontWrapJar>true</dontWrapJar>
  <headerType>gui</headerType>
  <!--<jar>.\lib\morpho.jar</jar>-->
  <classPath>
    <mainClass>edu.ucsb.nceas.morpho.Morpho</mainClass>
    <cp>lib\morpho.jar;lib\xercesImpl.jar;lib\xml-apis.jar;lib\xalan.jar;lib\morpho-config.jar;lib\jhall.jar;lib\shippedIcons.jar</cp>
    <cp>lib\morphohelp.jar;lib\httpclient.jar;lib\itislib.jar;lib\jsse.jar;lib\jcert.jar;lib\jnet.jar;lib\kunststoff.jar;lib\liveMap.jar;lib\utilities.jar;lib\dom4j.jar;lib\jcalendar-1.3.2.jar</cp>
  </classPath>
  <outfile>../../morpho.exe</outfile>
  <errTitle>Morpho</errTitle>
  <cmdLine></cmdLine>
  <chdir>.</chdir>
  <priority>normal</priority>
  <downloadUrl>http://java.com/download</downloadUrl>
  <supportUrl></supportUrl>
  <customProcName>false</customProcName>
  <stayAlive>false</stayAlive>
  <manifest></manifest>
  <icon>../../lib/DesktopIcon32x32.ico</icon>
  <jre>
    <path></path>
    <minVersion>1.5.0</minVersion>
    <maxVersion></maxVersion>
    <jdkPreference>preferJre</jdkPreference>
    <opt>-Xmx512m</opt>
    <opt>-Xss5m</opt>
    <opt>-Djava.endorsed.dirs=&quot;%EXEDIR%/lib/jar/apache&quot;</opt>
    <opt>-Djava.library.path=&quot;%EXEDIR%/lib&quot;</opt>
  </jre>
</launch4jConfig>
