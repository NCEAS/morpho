#!/bin/sh
#
# Shell script for launching morpho from unix systems
#
# '$RCSfile: morpho,v $'
# '$Author: jones $'
# '$Date: 2002-05-10 18:44:47 $'
# '$Revision: 1.12 $'

XMLP=./lib/xercesImpl.jar
API=./lib/xmlParserAPIs.jar
XALAN=./lib/xalan.jar
DMC=./lib/morpho.jar
CFG=./lib/morpho-config.jar
JLF=./lib/jlfgr-1_0.jar
HTP=./lib/httpclient.jar
ITS=./lib/itislib.jar
JSE=./lib/jsse.jar
JCE=./lib/jcert.jar
JNE=./lib/jnet.jar
CPATH=$XMLP:$API:$XALAN:$DMC:$CFG:$JLF:$HTP:$ITS:$JSE:$JCE:$JNE
java -Xmx512m -cp $CPATH edu.ucsb.nceas.morpho.framework.ClientFramework
