#!/bin/sh
#
# Shell script for launching morpho from unix systems
#
# '$RCSfile: morpho,v $'
# '$Author: jones $'
# '$Date: 2001-04-25 23:20:00 $'
# '$Revision: 1.2 $'

XMLP=./lib/xerces.jar
XALAN=./lib/xalan.jar
XML4J=./lib/xml4j.jar
SYM=./lib/sym.jar
DMC=./lib/dmanclient.jar
CPATH=.:$XMLP:$XALAN:$XML4J:$SYM:$DMC
java -cp $CPATH edu.ucsb.nceas.dtclient.ClientFramework
