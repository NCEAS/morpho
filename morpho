#!/bin/sh
#
# Shell script for launching morpho from unix systems
#
# '$RCSfile: morpho,v $'
# '$Author: jones $'
# '$Date: 2001-07-13 23:22:58 $'
# '$Revision: 1.6 $'

XMLP=./lib/xerces.jar
XALAN=./lib/xalan.jar
DMC=./lib/morpho.jar
JLF=./lib/jlfgr-1_0.jar
CPATH=$XMLP:$XALAN:$DMC:$JLF
java -cp $CPATH edu.ucsb.nceas.morpho.framework.ClientFramework
