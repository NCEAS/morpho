#!/bin/sh
#
# Shell script for launching morpho from unix systems
#
# '$RCSfile: morpho,v $'
# '$Author: higgins $'
# '$Date: 2002-03-12 23:57:19 $'
# '$Revision: 1.10 $'

XMLP=./lib/xerces.jar
XALAN=./lib/xalan.jar
DMC=./lib/morpho.jar
JLF=./lib/jlfgr-1_0.jar
HTP=./lib/httpclient.jar
ITS=./lib/itislib.jar
JSE=./lib/jsse.jar
JCE=./lib/jcert.jar
JNE=./lib/jnet.jar
CPATH=$XMLP:$XALAN:$DMC:$JLF:$HTP:$ITS:$JSE:$JCE:$JNE
java -Xmx128m -cp $CPATH edu.ucsb.nceas.morpho.framework.ClientFramework
