#!/bin/sh
#
# Shell script for launching morpho from unix systems
#
# '$RCSfile: morpho,v $'
# '$Author: jones $'
# '$Date: 2001-05-03 17:16:20 $'
# '$Revision: 1.4 $'

XMLP=./lib/xerces.jar
XALAN=./lib/xalan.jar
XML4J=./lib/xml4j.jar
SYM=./lib/sym.jar
DMC=./lib/morpho.jar
CPATH=.:$XMLP:$XALAN:$XML4J:$SYM:$DMC
java -cp $CPATH edu.ucsb.nceas.morpho.framework.ClientFramework
