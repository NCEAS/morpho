<?xml version="1.0"?>
<root>
<plugin>edu.ucsb.nceas.morpho.query.QueryPlugin</plugin>
<plugin>edu.ucsb.nceas.morpho.datapackage.DataPackagePlugin</plugin>
<MetaCatServletURL>http://dev.nceas.ucsb.edu/jones/servlet/metacat</MetaCatServletURL>
<local_dtd_directory>./lib/catalog</local_dtd_directory>
<local_xml_directory>./XMLworkFolder</local_xml_directory>
<local_cache_directory>./cache</local_cache_directory>
<debug_level>9</debug_level>
<log_file>false</log_file>
<searchmetacat>true</searchmetacat>
<searchlocal>true</searchlocal>
<pathquery_config>
  <doctypes_searched><doctype>any</doctype></doctypes_searched>
  <doctypes_returned>
    <returndoc>-//NCEAS//eml-dataset-2.0//EN</returndoc>
    <returndoc>-//NCEAS//resource//EN</returndoc>
  </doctypes_returned>
  <fields_returned>
  <returnfield>dataset/title</returnfield>
  <returnfield>dataset/originator/responsibleParty/individualName/surName</returnfield>
  <returnfield>keyword</returnfield>
  </fields_returned>

  <doctypes_searched_default>
    <doctype_default>any</doctype_default>
  </doctypes_searched_default>
  <doctypes_returned_default>
    <returndoc_default>-//NCEAS//eml-dataset-2.0//EN</returndoc_default>
    <returndoc_default>-//NCEAS//resource//EN</returndoc_default>
  </doctypes_returned_default>
  <fields_returned_default>
    <returnfield_default>dataset/title</returnfield_default>
    <returnfield_default>dataset/originator/responsibleParty/individualName/surName</returnfield_default>
    <returnfield_default>keyword</returnfield_default>
  </fields_returned_default>
</pathquery_config>
<localdoctypes>
    <localdoctypename>resource</localdoctypename>
    <localdoctypedtd>resource.dtd</localdoctypedtd>
    <localdoctypename>eml-dataset</localdoctypename>
    <localdoctypedtd>eml-dataset.dtd</localdoctypedtd>
    <localdoctypename>eml-access</localdoctypename>
    <localdoctypedtd>eml-access.dtd</localdoctypedtd>
    <localdoctypename>eml-context</localdoctypename>
    <localdoctypedtd>eml-context.dtd</localdoctypedtd>
    <localdoctypename>eml-file</localdoctypename>
    <localdoctypedtd>eml-file.dtd</localdoctypedtd>
    <localdoctypename>eml-status</localdoctypename>
    <localdoctypedtd>eml-status.dtd</localdoctypedtd>
    <localdoctypename>eml-software</localdoctypename>
    <localdoctypedtd>eml-software.dtd</localdoctypedtd>
    <localdoctypename>eml-supplement</localdoctypename>
    <localdoctypedtd>eml-supplement.dtd</localdoctypedtd>
    <localdoctypename>eml-variable</localdoctypename>
    <localdoctypedtd>eml-variable.dtd</localdoctypedtd>
</localdoctypes>
<saxparser>org.apache.xerces.parsers.SAXParser</saxparser>
<username>jones</username>
<accessionNumber>
  <scope>username</scope>
  <lastId>0</lastId>
  <separator>.</separator>
</accessionNumber>
<customFrames>
  <mainFrame>Resource</mainFrame>
  <frame>
    <frameName>Resource</frameName>
    <frameConfigFile>./lib/frames/resource.xml</frameConfigFile>
    <frameName>Entity</frameName>
    <frameConfigFile>./lib/frames/entity.xml</frameConfigFile>
  </frame>
</customFrames>
</root>