<?xml version="1.0"?>
<wizard size="500x500" dtd="eml-resource.dtd" doctype="-//NCEAS//eml-resource2.0//EN">
  <group label="Base Information" field="dataset" type="panel">
    <group size="480x100">
      <textbox label="Identifier" field="identifier" size="10" allowNull="no" editable="no" defaulttext="knb.1" />
      <textbox label="Short Name" field="shortName" size="10" />
      <textbox label="Title" field="title" size="10" allowNull="no"/>
    </group>
    
    <group label="Originator Information" type="panel" size="480x100">
      <group label="Originator" repeatable="yes" size="470x200" field="originator">
        <group label="Idenfication Information" repeatable="yes" allowNull="no" size="150x250"> 
            <textbox label="Salutation" field="salutation" size="5" repeatable="yes"/>
            <textbox label="Given Name" field="givenName" size="10" repeatable="yes"/>
            <textbox label="Sur Name" field="surName" size="10" allowNull="no"/>
          
          <textbox label="Organization" field="organizationName" size="10" repeatable="yes"/>
          <textbox label="Position" field="positionName" size="10" />
        </group>
        
      </group>
    </group>
  </group>
</wizard>
