<?xml version="1.0"?>
<!DOCTYPE eml-variable PUBLIC "-//NCEAS//eml-variable//EN" "file:///dmanclient/catalog/eml-variable.dtd" >
<eml-variable editor="edu.ucsb.nceas.morpho.editor.VariablePanel">
  <meta_file_id help="Unique identifier of this metadata record"></meta_file_id>
  <variable help="Variable information">
    <variable_name help="Unique variable name or code"></variable_name>
    <variable_definition help="Precise definition of variables in data set"></variable_definition>
    <unit help="Unit"></unit>
    <storage_type help="Storage type; Integer, floating point, character, string"></storage_type>
    <numeric_range help="Range for numeric values">
      <minimum help="Minimum value"></minimum>
      <maximum help="Maximum value"></maximum>
    </numeric_range>
    <missing_value_code help="Character used to represent missing data"></missing_value_code>
    <precision help="number of significant digits"></precision>
    <field_format help="Data format">
      <!--(variable_width|fixed_width)-->
      <variable_width>
        <delimiter help="Character used to delimit end of field"></delimiter>
      </variable_width>
    </field_format>
  </variable>
</eml-variable>
