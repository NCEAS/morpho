<mappingList>
   <!-- for metadata-->
   <mapping>
    <root>/eml:eml/dataset/</root>
    <xpath>title</xpath>
    <xpath>abstract</xpath>
    <loadDataFromRootPath>false</loadDataFromRootPath>
    <wizardPageClass>edu.ucsb.nceas.morpho.plugins.datapackagewizard.pages.General</wizardPageClass>
  </mapping>
  <mapping>
    <root>/eml:eml/dataset/keywordSet/</root>
    <xpath>keyword</xpath>
    <xpath>keywordThesaurus</xpath>
    <loadDataFromRootPath>true</loadDataFromRootPath>
