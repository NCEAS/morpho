<?xml version="1.0"?>
<wizard size="1000x1000" dtd="eml-resource.dtd">
  <group field="dataset" type="pathgrouping">
    <group label="Resource Information" type="panel">
      <group label="Originator" field="originator" repeatable="yes" type="pathgrouping">
        
        <group field="individualName">
          <textbox label="Salutation" field="salutation" size="5" />
          <textbox label="Given Name" field="givenName" size="10" />
          <textbox label="Sur Name" field="surName" size="10" />
          <textbox label="User Id" field="userid" size="10" />
          <textbox label="User Id Authentication System" field="@systemType" size="10" />
        </group>
        <group label="Contact Information" field="contactInfo" repeatable="true" type="pathgrouping">
          <textbox label="Voice Phone" field="phone/voice" size="10" />
          <textbox label="Fax" field="phone/facsimile" size="10" />
          <textbox label="Address" field="address/deliveryPoint" size="10" />
          <textbox label="City" field="address/city" size="10" />
          <textbox label="State" field="address/administrativeArea" size="10" />
          <textbox label="ZIP" field="address/postalCode" size="10" />
          <textbox label="Email address" field="address/electronicMailAddress" size="10" />
          <textbox label="Web Site" field="onlineResource/linkage/URL" size="10" allowNul="false" />
          <textbox label="Contact Instructions" field="contactInstructions" size="30" />
        </group>
      </group>
    </group>
    <group label="Coverage" type="panel">
      <group label="Temporal Coverage" field="temporalCoverage" repeatable="true" type="pathgrouping">
        <group label="Start Date" field="startDate" repeatable="false">
          <textbox label="Year" field="year" size="2" />
          <textbox label="Month" field="month" size="2" />
          <textbox label="Day" field="day" size="2" />
          <textbox label="Hour" field="hour" size="2" />
          <textbox label="Minute" field="minute" size="2" />
          <textbox label="Second" field="second" size="2" />
          <textbox label="Second Fraction" field="secondFraction" size="2" />
          <textbox label="Time Zone" field="timeZoneDesignator" size="5" />
        </group>
        <group label="Stop Date" field="stopDate" repeatable="false" type="pathgrouping">
          <textbox label="Year" field="year" size="2" />
          <textbox label="Month" field="month" size="2" />
          <textbox label="Day" field="day" size="2" />
          <textbox label="Hour" field="hour" size="2" />
          <textbox label="Minute" field="minute" size="2" />
          <textbox label="Second" field="second" size="2" />
          <textbox label="Second Fraction" field="secondFraction" size="2" />
          <textbox label="Time Zone" field="timeZoneDesignator" size="5" />
        </group>
      </group>
    </group>
  </group>
</wizard>
