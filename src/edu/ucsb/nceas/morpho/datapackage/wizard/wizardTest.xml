<?xml version="1.0"?>
<wizard size="800x600" dtd="eml-resource.dtd">
  <group field="dataset" type="pathgrouping">
    
    <group label="Resource Information" type="panel">
      <group label="Base Information">
        <textbox label="Identifier" field="identifier" size="10" allowNull="false"/>
        <textbox label="Resource Identifier" field="resourceIdentifier" size="10" allowNull="false" />
        <textbox label="Title" field="title" size="10" allowNull="false"/>
        <textbox label="Alternate Title" field="alternateTitle" size="10" />
        <combobox label="Dataset Representation Form" field="geoForm" size="10">
          <item name="Atlas" value="atlas" />
          <item name="Audio" value="audio" />
          <item name="Diagram" value="diagram" />
          <item name="Document" value="document" />
          <item name="Glove" value="globe" />
          <item name="Map" value="map" />
          <item name="Model" value="model" />
          <item name="Multimedia Presentation" value="multimediaPresenation" />
          <item name="Profile" value="profile" />
          <item name="Digital Raster Data" value="rasterDigitalData" />
          <item name="Remote Sensing Image" value="remoteSensingImage" />
          <item name="Section" value="section" />
          <item name="Spreadsheet" value="spreadsheet" />
          <item name="Digital Tabular Data" value="tabularDigitalData" />
          <item name="Digital Vector Data" value="vectorDigitalData" />
          <item name="Video" value="video" />
          <item name="View" value="view" />
        </combobox>
      </group>
      <group></group>
      <group></group>
    </group>
    <group label="Originator" field="originator" repeatable="yes" type="panel" allowNull="false">
      <group label="Originator Information">
        <combobox label="Originators Role" field="roleCode" size="10" allowNull="false" repeatable="yes">
          <item name="Content Provider" value="contentProvider"/>
          <item name="Custodian Steward" value="custodianSteward"/>
          <item name="Owner" value="owner"/>
          <item name="Distributor" value="distributor"/>
          <item name="Metadata Provider" value="metadataProvider"/>
          <item name="Originator" value="originator"/>
          <item name="Point of Contact" value="pointOfContact"/>
          <item name="PI" value="principalInvestigator"/>
          <item name="Data Processor" value="processor"/>
          <item name="Publisher" value="publisher"/>
          <item name="Author" value="author"/>
          <item name="Editor" value="editor"/>
        </combobox>
        <textbox label="Salutation" field="individualName/salutation" size="5" />
        <textbox label="Given Name" field="individualName/givenName" size="10" />
        <textbox label="Sur Name" field="individualName/surName" size="10" allowNull="false"/>
        <textbox label="User Id" field="individualName/userid" size="10" />
        <textbox label="User Id Authentication System" field="individualName/@systemType" size="10" />
        <textbox label="Organization" field="organizationName" size="10" repeatable="yes"/>
        <textbox label="Position" field="positionName" size="10" />
        
      </group>
      <group label="Contact Information" field="contactInfo" repeatable="yes" type="pathgrouping">
        <textbox label="Voice Phone" field="phone/voice" size="10" />
        <textbox label="Fax" field="phone/facsimile" size="10" />
        <textbox label="Address" field="address/deliveryPoint" size="10" />
        <textbox label="City" field="address/city" size="10" />
        <textbox label="State" field="address/administrativeArea" size="10" />
        <textbox label="ZIP" field="address/postalCode" size="10" />
        <textbox label="Email address" field="address/electronicMailAddress" size="10" />
        <textbox label="Web Site" field="onlineResource/linkage/URL" size="10" allowNull="false" repeatable="yes"/>
        <textbox label="Contact Instructions" field="contactInstructions" size="30" />
      </group>
    </group>
    
    <group label="Publication Information" type="panel">
      <group type="pathgrouping">
        <textbox label="Publication Date" field="pubdate" size="10" />
        <textbox label="Publication Place" field="pubplace" size="10" />
        <textbox label="Series" field="series" size="10" />
        <textbox label="Abstract" field="abstract" size="30" />
        <textbox label="Associated URL" field="URL" size="10" />
        <textbox label="Additional Information" field="additionalInfo" size="30" repeatable="yes"/>
      </group>
    </group>
    
    <group label="Coverage" type="panel">
      <group label="Temporal Coverage" field="temporalCoverage" repeatable="no" type="pathgrouping">
        <group label="Start Date" field="startDate" repeatable="yes" type="pathgrouping">
          <textbox label="Year" field="year" size="2" />
          <textbox label="Month" field="month" size="2" />
          <textbox label="Day" field="day" size="2" />
          <textbox label="Hour" field="hour" size="2" />
          <textbox label="Minute" field="minute" size="2" />
          <textbox label="Second" field="second" size="2" />
          <textbox label="Second Fraction" field="secondFraction" size="2" />
          <textbox label="Time Zone" field="timeZoneDesignator" size="5" />
        </group>
        <group label="Stop Date" field="stopDate" repeatable="no" type="pathgrouping">
          <textbox label="Year" field="year" size="2" />
          <textbox label="Month" field="month" size="2" />
          <textbox label="Day" field="day" size="2" />
          <textbox label="Hour" field="hour" size="2" />
          <textbox label="Minute" field="minute" size="2" />
          <textbox label="Second" field="second" size="2" />
          <textbox label="Second Fraction" field="secondFraction" size="2" />
          <textbox label="Time Zone" field="timeZoneDesignator" size="5" />
        </group>
      </group> 
    </group>
  
  </group>
</wizard>
